<!DOCTYPE html>
<html>
<head>
  <!-- Title here -->
  <title>@ViewBag.Title - DateMePlease.com</title>
  <!-- Description, Keywords and Author -->
  <meta name="description" content="Your description">
  <meta name="keywords" content="Your,Keywords">
  <meta name="author" content="ResponsiveWebInc">

  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Stylesheets -->
  <!-- Bootstrap CSS -->
  <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
  @*<link href="~/Content/theme/css/bootstrap.min.css" rel="stylesheet">*@
  <!-- Pretty Photo CSS -->
  <link href="~/Content/theme/css/prettyPhoto.css" rel="stylesheet">
  <!-- Font awesome CSS -->
  <link href="~/Content/theme/css/font-awesome.min.css" rel="stylesheet">
  <!-- Change the template color below. Supported colors blue,red and green. -->
  <link href="~/Content/theme/css/red.css" rel="stylesheet">
  <!-- Custom CSS -->
  <link href="~/Content/theme/css/style.css" rel="stylesheet">

  <!-- Favicon -->
  <link rel="shortcut icon" href="~/Content/theme/img/favicon/favicon.png">

  <link href="~/Content/Site.css" rel="stylesheet" />

  @RenderSection("header", false)

</head>
<body>


  <!-- Header -->
  <!-- Fixed Bar -->

  <div class="fixedbar">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="logo">
            <div class="pull-left">
              <!-- Logo with font awesome icon. Replace it with your logo. Replace # with your site link -->
              <h1><a href='@Url.Action("Index", "Home")'><i class="fa fa-heart"></i> DateMePlease.com</a></h1>
            </div>
            <div class="navbar" role="navigation">
              <!-- Brand and toggle get grouped for better mobile display -->
              <div class="navbar-header">
                <button type="button" class="navbar-toggle br-orange" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
              </div>

              <!-- Navigation Starts Here -->
              <div class="collapse navbar-collapse">
                <!-- Main navigation -->
                <ul class="nav navbar-nav navbar-left">
                  <li>
                    <a href='@Url.Action("Index", "Home")' rel="ddsubmenu1">Home</a>
                  </li>
                  <li><a href='@Url.Action("About", "Home")'>About Us</a></li>
                  <li><a href='@Url.Action("Contact", "Home")'>Contact Us</a></li>
                </ul>
                @Html.Partial("_LoginPartial")
              </div><!--/ Navigation Ends Here -->

            </div>
          </div>
        </div>
        <div class="clearfix"></div>
      </div>
    </div>
  </div>

  <!-- Fixed Bar End -->
  <!-- Main content -->

  <div class="main">
    <div class="container">
      <div class="row">

        @RenderBody()

      </div>
    </div>
  </div>

  <!-- Foot -->
  <!-- Three column footer -->

  <div class="foot">
    <div class="container">
      <div class="row">
        <div class="col-md-4">
          <div class="padd">
            <!-- Add class "widget" -->
            <div class="widget">
              <!-- Title -->
              <h3><a href='@Url.Action("Index", "Home")'><i class="fa fa-heart"></i> DateMePlease.com</a></h3>
              <!-- Content -->
              <!-- Below is the blockquote -->
              <blockquote>
                
              </blockquote>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="padd">
            <div class="widget">
              <h3>Sed a interdum mauris</h3>
              <ul>
                <li><a href="#">Sed eu leo orci, condimentum gravida metus</a></li>
                <li><a href="#">Etiam at nulla ipsum, in rhoncus purus</a></li>
                <li><a href="#">Fusce vel magna faucibus felis dapibus facilisis</a></li>
                <li><a href="#">Vivamus scelerisque dui in massa</a></li>
                <li><a href="#">Pellentesque eget adipiscing dui semper</a></li>
              </ul>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="padd">
            <div class="widget">
              <h3>Service</h3>
              <ul>
                <li><a href="#">Sed eu leo gravida metus</a></li>
                <li><a href="#">Etiam at rhoncus purus</a></li>
                <li><a href="#">Fusce vel dapibus facilisis</a></li>
                <li><a href="#">Vivamus scelerisque dui in massa</a></li>
                <li><a href="#">Pellentesque eget  dui semper</a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Footer -->
  <footer>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <!-- Copyright information -->
          <div class="fpadd">Copyright &copy; 2015 - <a href="@Url.Action("Index", "Home")">PleaseDateMe.com</a> / <a href="@Url.Action("Index", "Home")">Disclaimer</a> / <a href="@Url.Action("Index", "Home")">Privacy Policy</a></div>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </footer>


  <!-- Scroll to top -->
  <span class="totop"><a href="#"><i class="fa fa-angle-up"></i></a></span>


  <!-- JavaScript files -->
  <!-- jQuery -->
  <script src="~/Scripts/jquery-2.1.3.min.js"></script>
  @*<script src="~/Content/theme/js/jquery.js"></script>*@
  <!-- Bootstrap JS -->
  <script src="~/Scripts/bootstrap.min.js"></script>
  @*<script src="~/Content/theme/js/bootstrap.min.js"></script>*@
  <!-- Easing JS -->
  <script src="~/Content/theme/js/easing.js"></script>
  <!-- Pretty Photo, Isotope JS -->
  <script src="~/Content/theme/js/jquery.prettyPhoto.js"></script>
  <script src="~/Content/theme/js/isotope.js"></script>
  <!-- Respond JS for IE8 -->
  <script src="~/Content/theme/js/respond.min.js"></script>
  <!-- HTML5 Support for IE -->
  <script src="~/Content/theme/js/html5shiv.js"></script>
  <!-- Custom JS -->
  <script src="~/Content/theme/js/custom.js"></script>


  @RenderSection("scripts", required: false)
</body>
</html>
